{
  "npmScope": "e-square",
  "affected": { "defaultBase": "main" },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": { "dependencies": "*", "devDependencies": "*" },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    ".eslintrc.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": { "cacheableOperations": ["build", "lint", "test", "e2e"] }
    }
  },
  "projects": {
    "ddd": { "tags": [] },
    "ddd-e2e": { "tags": [], "implicitDependencies": ["ddd"] },
    "semantic-release": { "tags": [] },
    "semantic-release-e2e": {
      "tags": [],
      "implicitDependencies": ["semantic-release"]
    }
  },
  "workspaceLayout": { "appsDir": "e2e", "libsDir": "packages" }
}
